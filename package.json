{
  "name": "@vexcited/eslint-config",
  "type": "module",
  "version": "0.1.0",
  "description": "Vexcited's ESLint config",
  "author": "Mikkel ALMONTE--RINGAUD <contact@vexcited.com> (https://github.com/Vexcited)",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Vexcited",
  "homepage": "https://github.com/Vexcited/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vexcited/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/Vexcited/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
